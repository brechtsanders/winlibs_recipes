wl-download -v -d $TARBALLDIR/$BASENAME $DOWNLOADSOURCEURL
wl-wait4deps
wl-showstatus extract
tar xj --force-local -f $TARBALLDIR/$BASENAME/okteta-v$VERSION.tar.bz2
cd okteta-v$VERSION
mkdir -p build_win &&
 wl-showstatus configure &&
