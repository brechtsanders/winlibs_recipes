export DEPENDANCIES=gettext,qt,libkf5activities,libkf5attica,libkf5auth,libkf5bookmarks,libkf5codecs,libkf5completion,libkf5configwidgets,libkf5config,libkf5coreaddons,libkf5crash,libkf5dbusaddons,libkf5doctools,libkf5guiaddons,libkf5i18n,libkf5iconthemes,libkf5itemmodels,libkf5itemviews,libkf5jobwidgets,libkf5kio,libkf5newstuff,libkf5package,libkf5parts,libkf5plasma-framework,libkf5service,libkf5solid,libkf5sonnet,libkf5syntax-highlighting,libkf5texteditor,libkf5textwidgets,libkf5wallet,libkf5widgetsaddons,libkf5windowsystem,libkf5xmlgui,libkf5userfeedback
export OPTIONALDEPENDANCIES=
export BUILDDEPENDANCIES=cmake,extra-cmake-modules,ninja
#export LICENSEFILE=LICENSES/*
export LICENSEFILE=LICENSES/LGPL-2.0-or-later.txt
export LICENSETYPE=LGPL
export DOWNLOADURL="https://invent.kde.org/utilities/kate/-/tags"
export INSTALLPREFIX=`pwd`/inst_$BASENAME-$VERSION
export DOWNLOADSOURCEURL=https://invent.kde.org/utilities/kate/-/archive/v$VERSION/kate-v$VERSION.tar.bz2
wl-showstatus download
wl-download -v -d $TARBALLDIR/$BASENAME $DOWNLOADSOURCEURL
wl-wait4deps
wl-showstatus extract
tar xfj $TARBALLDIR/$BASENAME/kate-v$VERSION.tar.bz2
cd kate-v$VERSION
mkdir -p build_win &&
 wl-showstatus configure &&
 PATH=$PATH:$MINGWPREFIX/qt/bin QTDIR=$MINGWPREFIX/qt QMAKESPEC=$MINGWPREFIX/qt/mkspecs/win32-g++ cmake.exe -Wno-dev -GNinja -DCMAKE_INSTALL_PREFIX:PATH=$INSTALLPREFIX -DCMAKE_BUILD_TYPE:STRING=Release -DKDE_L10N_BRANCH:STRING=stable -DKI18N_PYTHON_EXECUTABLE=FILEPATH=$PYDIR/python.exe -S. -Bbuild_win &&
 #wl-showstatus build &&
 #make -Cbuild_win &&
 wl-showstatus build-install &&
 ( PATH=$PATH:$MINGWPREFIX/qt/bin ninja -Cbuild_win install/strip || PATH=$PATH:$MINGWPREFIX/qt/bin ninja -j1 -Cbuild_win install/strip ) &&
 wl-makepackage -c -d && wl-install -d $BASENAME-$VERSION && cd .. && wl-showstatus cleanup && rm -rf kate-v$VERSION
