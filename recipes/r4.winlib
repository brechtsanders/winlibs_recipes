export NAME="r4"
export STATUS=
export URL=https://github.com/cppfw/r4
export BASENAME=r4
export DESCRIPTION="3D math library in C++"
export CATEGORY=math
export TYPE=library
export VERSION=1.0.71
export VERSIONDATE=20230320
wl-showstatus --package-version
export DEPENDENCIES=papki
export OPTIONALDEPENDENCIES=
export BUILDDEPENDENCIES=prorab
export OPTIONALBUILDDEPENDENCIES=
export LICENSEFILE=LICENSE
export LICENSETYPE=MIT
export DOWNLOADURL="https://github.com/cppfw/r4/tags"
export INSTALLPREFIX=`pwd`/inst_$BASENAME-$VERSION
export DOWNLOADSOURCEURL=https://github.com/cppfw/r4/archive/refs/tags/$VERSION.tar.gz
wl-showstatus download
wl-download -v -d $TARBALLDIR/$BASENAME $DOWNLOADSOURCEURL
wl-wait4deps
wl-showstatus extract
tar xz --force-local -f $TARBALLDIR/$BASENAME/$VERSION.tar.gz
cd $BASENAME-$VERSION
touch prorab-test.mk prorab-clang-format.mk prorab-license.mk prorab-doxygen.mk
wl-showstatus build &&
 make -I$MINGWPREFIX/share/prorab install this_lint_cmd= CC=${CC:-gcc} AR=${AR:-ar} PREFIX=$INSTALLPREFIX &&
 wl-makepackage -c -d && wl-install -d $BASENAME-$VERSION && cd .. && wl-showstatus cleanup && rm -rf $BASENAME-$VERSION



