export DOWNLOADURL="https://github.com/BelledonneCommunications/belr/tags"
export INSTALLPREFIX=`pwd`/inst_$BASENAME-$VERSION
export DOWNLOADSOURCEURL=https://github.com/BelledonneCommunications/belr/archive/refs/tags/$VERSION.tar.gz
wl-showstatus download
wl-download -v -d $TARBALLDIR/$BASENAME $DOWNLOADSOURCEURL
wl-wait4deps
wl-showstatus extract
tar xz --force-local -f $TARBALLDIR/$BASENAME/$VERSION.tar.gz
cd $BASENAME-$VERSION
# fix undefined BCTBX_UNUSED in src/belr.cpp (version >= 5.2.25)
mv src/belr.cpp src/belr.cpp.bak &&
echo "#define BCTBX_UNUSED(arg) arg" > src/belr.cpp &&
cat src/belr.cpp.bak >> src/belr.cpp
# fix undefined BCTBX_UNUSED in src/grammarbuilder.cpp (version >= 5.2.25)
mv src/grammarbuilder.cpp src/grammarbuilder.cpp.bak &&
echo "#define BCTBX_UNUSED(arg) arg" > src/grammarbuilder.cpp &&
cat src/grammarbuilder.cpp.bak >> src/grammarbuilder.cpp
mkdir -p build_static build_shared &&
 wl-showstatus configure &&
