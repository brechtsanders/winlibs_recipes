export NAME="svgdom"
export STATUS=
export URL=https://github.com/cppfw/svgdom
export BASENAME=svgdom
export DESCRIPTION="SVG document object model in C++"
export CATEGORY=fileformat
export TYPE=library
#export VERSION=0.4.6
#export VERSIONDATE=20230320
