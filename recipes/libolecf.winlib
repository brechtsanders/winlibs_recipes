export NAME="libolecf"
export STATUS=
export URL=https://github.com/libyal/libolecf
export BASENAME=libolecf
export DESCRIPTION="Library and tools to access the OLE 2 Compound File (OLECF) format"
export CATEGORY=development
export TYPE=library
