 #auto/configure --prefix=$INSTALLPREFIX --with-cc=gcc --with-pcre=$MINGWPREFIX --with-zlib=$MINGWPREFIX --with-openssl=$MINGWPREFIX --with-libatomic=$MINGWPREFIX &&
