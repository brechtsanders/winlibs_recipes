#cd gnurl-$(echo $VERSION|sed -e "s/\./_/g")
cd gnurl-$VERSION
# fix undefined int64_t in lib/http.h (version >= 7.72.0)
mv lib/http.h lib/http.h.bak &&
echo "#include <stdint.h>" > lib/http.h &&
cat lib/http.h.bak >> lib/http.h
wl-showstatus configure &&
