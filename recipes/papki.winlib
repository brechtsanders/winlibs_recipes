export NAME="papki"
export STATUS=
export URL=https://github.com/cppfw/papki
export BASENAME=papki
export DESCRIPTION="File system abstraction in C++"
export CATEGORY=development
export TYPE=library
export VERSION=1.0.126
export VERSIONDATE=20230320
wl-showstatus --package-version
export DEPENDENCIES=zlib
export OPTIONALDEPENDENCIES=
export BUILDDEPENDENCIES=prorab
export OPTIONALBUILDDEPENDENCIES=
export LICENSEFILE=LICENSE
export LICENSETYPE=MIT
export DOWNLOADURL="https://github.com/cppfw/papki/tags"
export INSTALLPREFIX=`pwd`/inst_$BASENAME-$VERSION
export DOWNLOADSOURCEURL=https://github.com/cppfw/papki/archive/refs/tags/$VERSION.tar.gz
wl-showstatus download
wl-download -v -d $TARBALLDIR/$BASENAME $DOWNLOADSOURCEURL
wl-wait4deps
wl-showstatus extract
tar xz --force-local -f $TARBALLDIR/$BASENAME/$VERSION.tar.gz
cd $BASENAME-$VERSION
touch prorab-test.mk prorab-clang-format.mk prorab-install-dbgsrc.mk prorab-license.mk prorab-pkg-config.mk prorab-doxygen.mk
wl-showstatus build-install &&
 make -I$MINGWPREFIX/share/prorab install prorab_lint_cmd_clang_tidy= CC=${CC:-gcc} AR=${AR:-ar} PREFIX=$INSTALLPREFIX &&
 wl-makepackage -c -d && wl-install -d $BASENAME-$VERSION && cd .. && wl-showstatus cleanup && rm -rf $BASENAME-$VERSION



