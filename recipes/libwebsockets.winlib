cat lib/roles/http/client/client-handshake.c.bak >> lib/roles/http/client/client-handshake.c
# fix missing bzero (version >= 3.1.0)
for F in $(grep -l bzero $(find -name '*.c')); do
 mv $F $F.bak &&
 echo "#define bzero(b,len) memset(b, 0, len)" > $F &&
 cat $F.bak >> $F
done
