export NAME="filevercmp"
export STATUS=
export URL=https://github.com/ekg/filevercmp
export BASENAME=filevercmp
export DESCRIPTION="Compare version strings"
export CATEGORY=dataprocessing
export TYPE=library
export VERSION=master
export VERSIONDATE=20201122
wl-showstatus --package-version
export DEPENDENCIES=
export OPTIONALDEPENDENCIES=
export BUILDDEPENDENCIES=
export OPTIONALBUILDDEPENDENCIES=
export LICENSEFILE=
export LICENSETYPE=
export DOWNLOADURL="https://github.com/ekg/filevercmp/releases/"
export INSTALLPREFIX=`pwd`/inst_$BASENAME-$VERSION
export DOWNLOADSOURCEURL=https://github.com/ekg/filevercmp/archive/$VERSION.tar.gz
wl-showstatus download
wl-download -v -d $TARBALLDIR/$BASENAME $DOWNLOADSOURCEURL
wl-wait4deps
wl-showstatus extract
tar xfz $TARBALLDIR/$BASENAME/$VERSION.tar.gz
cd $BASENAME-$VERSION
wl-showstatus build &&
 gcc -c -o filevercmp.o filevercmp.c -I. -O3 &&
 ar cru libfilevercmp.a filevercmp.o &&
 ( echo "EXPORTS" && nm -f posix --defined-only -p libfilevercmp.a | sed -n -e "s/^_*\([^ ]*\) T .*$/\1/p" ) > filevercmp.def &&
 gcc -shared -s -mwindows -def filevercmp.def -o filevercmp.dll libfilevercmp.a -Wl,--out-implib,libfilevercmp.dll.a &&
 wl-showstatus install &&
 mkdir -p $INSTALLPREFIX/include $INSTALLPREFIX/lib $INSTALLPREFIX/bin &&
 cp -f *.h $INSTALLPREFIX/include/ &&
 cp -f *.a *.def $INSTALLPREFIX/lib/ &&
 cp -f *.dll $INSTALLPREFIX/bin/ &&
 wl-makepackage -c -d && wl-install -d $BASENAME-$VERSION && cd .. && wl-showstatus cleanup && rm -rf $BASENAME-$VERSION



