 # create .config file
 sed -e "s/^\(CONFIG_CROSS_COMPILER_PREFIX=\).*$/\1\"$RUNPLATFORM\-\"/" configs/mingw64_defconfig > .config &&
 echo "CONFIG_FEATURE_IP_LINK_CAN=n" >> .config &&
