# create missing tutor language folder (version >= 9.1.0848)
sed -i.bak2 -e "s?^\(\s*\)chmod \$(DIRMOD) \(\$(DEST_TUTOR)/[^\s]*\)?\1mkdir -p \2\n&?" src/Makefile
