export NAME="squashfuse"
export STATUS=
export URL=https://github.com/vasi/squashfuse
export BASENAME=squashfuse
export DESCRIPTION="FUSE filesystem to mount squashfs archives"
export CATEGORY=system,fileformat
export TYPE=library
