export NAME="Yue"
export STATUS=
export URL=https://libyue.com/
export BASENAME=yue
export DESCRIPTION="A library for creating native cross-platform GUI apps"
export CATEGORY=gui
export TYPE=library
#export VERSION=0.11.0
#export VERSIONDATE=20230209
#export VERSION=0.12.0
