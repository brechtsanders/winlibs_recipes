#ifdef __MINGW32__
int APIENTRY wWinMain(HINSTANCE hInstance, HINSTANCE hPrevInstance, PWSTR pCmdLine, int nCmdShow)
{
        int argc;
        LPWSTR* argv;
        if ((argv = CommandLineToArgvW(pCmdLine, &argc)) == NULL)
                return 1;
        return wmain(argc, argv, GetEnvironmentStringsW());
}
