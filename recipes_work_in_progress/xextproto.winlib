export NAME="X.Org xextproto"
export STATUS=
#export URL=http://www.freedesktop.org/wiki/Software/xlibs
export URL=http://www.x.org/
export BASENAME=xextproto
export DESCRIPTION="Common X Extensions library."
export CATEGORY=x
export TYPE=library
#export VERSION=7.0.5
