export DOWNLOADSOURCEURL=http://ftp.gnome.org/pub/gnome/sources/$BASENAME/`echo $VERSION|sed 's/^\([0-9]*\.[0-9]*\)\..*$/\1/'`/$BASENAME-$VERSION.tar.xz
#export DOWNLOADSOURCEURL=https://download.gnome.org/sources/totem/$(echo $VERSION|sed 's/^\([0-9]*\)\..*$/\1/')/$BASENAME-$VERSION.tar.xz
wl-showstatus download
wl-download -v -d $TARBALLDIR/$BASENAME $DOWNLOADSOURCEURL
wl-wait4deps
#tar xfj $TARBALLDIR/$BASENAME/$BASENAME-$VERSION.tar.bz2
tar xfJ $TARBALLDIR/$BASENAME/$BASENAME-$VERSION.tar.xz
cd $BASENAME-$VERSION
# fix missing gdk/gdkx.h
mkdir -p gdk
touch gdk/gdkx.h
# fix missing GTK_IS_PLUG in src/backend/bacon-video-widget-gst-0.10.c
mv src/backend/bacon-video-widget-gst-0.10.c src/backend/bacon-video-widget-gst-0.10.c.bak
echo "#include <gtk/gtkplug.h>" > src/backend/bacon-video-widget-gst-0.10.c
cat src/backend/bacon-video-widget-gst-0.10.c.bak >> src/backend/bacon-video-widget-gst-0.10.c
# skip making help
cat > help/Makefile.in << EOF
clean:
all:
install:
install-strip:
EOF
# fix DATADIR redefinition in src/plugins/youtube/totem-youtube.c
mv src/plugins/youtube/totem-youtube.c src/plugins/youtube/totem-youtube.c.bak
echo "#undef DATADIR" >  src/plugins/youtube/totem-youtube.c
cat  src/plugins/youtube/totem-youtube.c.bak >>  src/plugins/youtube/totem-youtube.c
# fix error in src/backend/bvw-test.c: /include/objidl.h:2477:5: error: expected identifier or '(' before string constant
cp src/backend/bvw-test.c src/backend/bvw-test.c.bak
echo "#undef DATADIR" > src/backend/bvw-test.c
cat src/backend/bvw-test.c.bak >> src/backend/bvw-test.c
# fix configure
patch -ulbf configure << EOF
--- configure  2010-09-27 14:42:20 +0200
+++ configure  2010-10-13 20:51:24 +0200
@@ -18436,13 +18436,2 @@

-       as_fn_error \$? "Package requirements (\$SMCLIENT_PKGS) were not met:
-
-\$SMCLIENT_PKG_ERRORS
-
-Consider adjusting the PKG_CONFIG_PATH environment variable if you
-installed software in a non-standard prefix.
-
-Alternatively, you may set the environment variables SMCLIENT_CFLAGS
-and SMCLIENT_LIBS to avoid the need to call pkg-config.
-See the pkg-config man page for more details." "\$LINENO" 5
-
 elif test \$pkg_failed = untried; then
EOF
# fix problem detecting GNU gettext tools on 64-bit
if ( echo $RUNPLATFORM | grep -q x86_64 ); then
 mv configure configure.bak
 sed -e "s/as_fn_error\(.*GNU gettext tools not found; required for intltool\)/as_echo\1/" configure.bak > configure
fi
# remove gdk-x11 dependancy
mv configure configure.bak
sed -e "s/gdk-x11/gdk-win32/" configure.bak > configure
# fix clutter-gst dependancy
mv configure configure.bak2
sed -e "s/clutter-gst-2/clutter-gst-3/" configure.bak2 > configure
wl-showstatus configure &&
