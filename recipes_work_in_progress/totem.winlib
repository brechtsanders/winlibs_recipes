 PATH=$PATH:$PERLDIR/../c/bin INTLTOOL_PERL="$PERLDIR/bin/perl.exe" ./configure --prefix=$INSTALLPREFIX --build=$BUILDPLATFORM --host=$RUNPLATFORM --with-smclient=win32 --without-x CFLAGS="`$PKG_CONFIG --cflags gtk+-2.0`" LDFLAGS="-Wl,-no-undefined" &&
 wl-showstatus build-install &&
 ( make install-strip || make install-strip LIBS="-Wl,--as-needed -lglib-2.0 -lgobject-2.0 -lgdk_pixbuf-2.0" ) &&
    echo OK
