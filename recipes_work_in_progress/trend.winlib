export NAME="Trend"
export STATUS=
export URL=https://www.thregr.org/~wavexx/software/trend/
export BASENAME=trend
export DESCRIPTION="trend is a general-purpose, efficient trend graph for "live" data. Data is read in ASCII form from a file or continuously from a FIFO and displayed in real-time into a multi-pass trend (much like a CRT oscilloscope). trend can be used as a rapid analysis tool for progressive or time-based data series together with trivialÂ scripting."
export CATEGORY=graphics,dataprocessing
export TYPE=library
export VERSION=1.4
export VERSIONDATE=20221215
wl-showstatus --package-version
export DEPENDENCIES=
export OPTIONALDEPENDENCIES=
export BUILDDEPENDENCIES=
export OPTIONALBUILDDEPENDENCIES=
export LICENSEFILE=COPYING.txt
export LICENSETYPE=LGPL
export DOWNLOADURL="https://www.thregr.org/~wavexx/software/trend/releases/"
export INSTALLPREFIX=`pwd`/inst_$BASENAME-$VERSION
export DOWNLOADSOURCEURL=https://www.thregr.org/~wavexx/software/trend/releases/$BASENAME-$VERSION.tar.gz
wl-showstatus download
wl-download -v -d $TARBALLDIR/$BASENAME $DOWNLOADSOURCEURL
wl-wait4deps
wl-showstatus extract
tar xfz $TARBALLDIR/$BASENAME/$BASENAME-$VERSION.tar.gz
cd $BASENAME-$VERSION
wl-showstatus build &&
 make -Csrc CXX=${CXX:-g++} CXXFLAGS="-fpermissive" &&
    echo OK
