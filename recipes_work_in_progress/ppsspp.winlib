export NAME="PPSSPP"
export STATUS=
export URL=https://github.com/hrydgard/ppsspp
export BASENAME=ppsspp
export DESCRIPTION="PPSSPP - a fast and portable PSP emulator"
export CATEGORY=emulation
export TYPE=application
