for D in bobcat/*; do
 if [ -f $D/$(basename $D) ]; then
  #cp -f $D/$(basename $D) include/bobcat/
  echo "#include \"../bobcat/$(basename $D)/$(basename $D)\"" > include/bobcat/$(basename $D)
 fi
done
wl-showstatus build &&
 touch SUCCESS &&
 for F in $(find -name '*.cc'); do
  if [ ! -f $F.o ]; then
   echo CXX $F
   ${CXX:-g++} -c -o $F.o $F -std=c++20 -Iinclude -I. -Dlstat=stat -Dstrcasestr=stristr || ( rm -f SUCCESS; false ) || break
  fi
 done &&
 rm SUCCESS &&
    echo OK

  ./configure --help 2> /dev/null
  cat meson_options.txt 2> /dev/null
  head COPYING* LICENSE* COPYRIGHT*
  ls -ld COPYING* LICENSE* COPYRIGHT* configure* m4 CMakeLists.txt cmake Makefile GNUmakefile setup.py scons SConscript SConstruct meson.build meson_options.txt *.pro build 2> /dev/null

