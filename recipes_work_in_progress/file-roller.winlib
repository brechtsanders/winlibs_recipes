EOF
# fix missing pwd.h (version >= 3.6.2)
cat > pwd.h << EOF
EOF
# fix missing _POSIX_ARG_MAX in src/fr-command.c (version >= 3.6.2)
mv src/fr-command.c src/fr-command.c.bak
echo "#define _POSIX_ARG_MAX 4096" > src/fr-command.c
cat src/fr-command.c.bak >> src/fr-command.c
# remove gio-unix dependancy
mv configure configure.bak
sed -e "s/gio-unix/gio-windows/g" configure.bak > configure
wl-showstatus configure &&
