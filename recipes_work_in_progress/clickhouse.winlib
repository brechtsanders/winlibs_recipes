patch -ulbf cmake/target.cmake << EOF
@@ -19,2 +19,5 @@
     add_definitions(-D OS_SUNOS)
+elseif (CMAKE_SYSTEM_NAME MATCHES "Windows")
+    set (OS_SUNOS 1)
+    add_definitions(-D OS_WINDOWS)
 else ()
EOF
# fix cmake/tools.cmake (version >= 24.3.2.23-lts)
patch -ulbf cmake/tools.cmake << EOF
@@ -7,3 +7,3 @@
 else ()
-    message (FATAL_ERROR "Compiler \${CMAKE_CXX_COMPILER_ID} is not supported")+    message (WARNING "Compiler \${CMAKE_CXX_COMPILER_ID} is not supported")
 endif ()
@@ -78,3 +78,3 @@
 elseif (NOT ARCH_S390X AND NOT OS_FREEBSD)
-    message (FATAL_ERROR "The only supported linker is LLVM's LLD, but we cannot find it.")
+    message (WARNING "The only supported linker is LLVM's LLD, but we cannot find it.")
 else ()
@@ -128,3 +128,3 @@
 else ()
-    message (FATAL_ERROR "Cannot find objcopy.")
+    message (WARNING "Cannot find objcopy.")
 endif ()
@@ -140,3 +140,3 @@
 else ()
-    message (FATAL_ERROR "Cannot find strip.")
+    message (WARNING "Cannot find strip.")
 endif ()
EOF

