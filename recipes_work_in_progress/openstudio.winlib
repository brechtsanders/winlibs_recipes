export NAME="OpenStudio"
export STATUS=
export URL=https://github.com/NREL/OpenStudio
#export URL=https://github.com/openstudiocoalition/OpenStudio
export BASENAME=openstudio
export DESCRIPTION="OpenStudio is a cross-platform collection of software tools to support whole building energy modeling using EnergyPlus and advanced daylight analysis using Radiance."
export CATEGORY=science
export TYPE=application
export VERSION=3.6.1
export VERSIONDATE=20230728
wl-showstatus --package-version
export DEPENDENCIES=energyplus
export OPTIONALDEPENDENCIES=
export BUILDDEPENDENCIES=cmake,ninja
export OPTIONALBUILDDEPENDENCIES=
export LICENSEFILE=LICENSE.md
export LICENSETYPE=
export DOWNLOADURL="https://github.com/NREL/OpenStudio/releases"
export INSTALLPREFIX=`pwd`/inst_$BASENAME-$VERSION
export DOWNLOADSOURCEURL=https://github.com/NREL/OpenStudio/archive/refs/tags/v$VERSION.tar.gz
wl-showstatus download
wl-download -v -d $TARBALLDIR/$BASENAME $DOWNLOADSOURCEURL
wl-wait4deps
wl-showstatus extract
tar xz --force-local -f $TARBALLDIR/$BASENAME/v$VERSION.tar.gz
cd OpenStudio-$VERSION
# fix CMakeLists.txt (version >= 3.6.1)
patch -ulbf CMakeLists.txt << EOF
@@ -441,3 +441,3 @@
 set(CONAN_INSTALL_FILE "\${PROJECT_SOURCE_DIR}/ConanInstall.cmake")
-include("\${CONAN_INSTALL_FILE}")
+#include("\${CONAN_INSTALL_FILE}")

EOF

#grep -wi add_library $(find -name CMakeLists.txt -or -name '*.cmake')
mkdir -p build_test && cmake.exe -G"MSYS Makefiles" -S. -Bbuild_test -LH && echo OK; rm -rf build_test

