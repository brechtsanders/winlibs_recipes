export NAME="kep3"
export STATUS=
export URL=https://esa.github.io/kep3/
export BASENAME=kep3
export DESCRIPTION="pykep is a coolbox developed at the European Space Agency by its Advanced Concepts Team. Its main purpose is to allow for fast prototyping of research ideas on interplanetary trajectory design. At the library core is the implementation of efficient algorithms allowing to solve the multiple revolutions Lambertâ€™s problem, low-thrust problems, multiple asteroid randezvous problems and more."
export CATEGORY=science
export TYPE=library
export VERSION=main
export VERSIONDATE=20231231
wl-showstatus --package-version
export DEPENDENCIES=boost,spdlog,mppp,heyoka,xtensor,xtensor-blas
export OPTIONALDEPENDENCIES=
export BUILDDEPENDENCIES=cmake,ninja
export OPTIONALBUILDDEPENDENCIES=
export LICENSEFILE=COPYING
export LICENSETYPE=MPL
export DOWNLOADURL="https://github.com/esa/kep3/tags"
export INSTALLPREFIX=`pwd`/inst_$BASENAME-$VERSION
export DOWNLOADSOURCEURL=https://github.com/esa/kep3/archive/refs/heads/$VERSION.tar.gz
wl-showstatus download
wl-download -v -d $TARBALLDIR/$BASENAME $DOWNLOADSOURCEURL
wl-wait4deps
wl-showstatus extract
tar xz --force-local -f $TARBALLDIR/$BASENAME/$VERSION.tar.gz
cd $BASENAME-$VERSION

#grep -wi add_library $(find -name CMakeLists.txt -or -name '*.cmake')
mkdir -p build_test && cmake.exe -G"MSYS Makefiles" -Dkep3_BUILD_BENCHMARKS:BOOL=OFF -Dkep3_BUILD_TESTS:BOOL=OFF -S. -Bbuild_test -LH && echo OK; rm -rf build_test
