wl-showstatus build &&
 make -Csrc CC=${CC:-gcc} CXX=${CXX:-g++} TARGET=winagent &&
 make -Csrc CC=${CC:-gcc} CXX=${CXX:-g++} TARGET=server &&
 make -Csrc CC=${CC:-gcc} CXX=${CXX:-g++} TARGET=local &&
